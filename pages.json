{
    "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
        {
            "path": "pages/index/index",
            "style": {
                "navigationBarTitleText": "uni-app"
            }
        },
        {
            "path": "pages/mine/mine",
            "style": {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
        },
        {
            "path": "pages/tim/record",
            "style": {
                "navigationBarTitleText": "uni-app"
            }
        },
        {
            "path": "pages/tim/room",
            "style": {
                "navigationBarTitleText": "聊天室"
            }
        }
    ],
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8"
    },
    "tabBar": {
        "list": [
            {
                "pagePath": "pages/index/index",
                "iconPath": "static/tab_index_unselected.png",
                "selectedIconPath": "static/tab_index_selected.png",
                "text": "首页"
            },
            {
                "pagePath": "pages/tim/record",
                "iconPath": "static/tab_msg_unselected.png",
                "selectedIconPath": "static/tab_msg_selected.png",
                "text": "消息"
            },
            {
                "pagePath": "pages/mine/mine",
                "iconPath": "static/tab_mine_unselected.png",
                "selectedIconPath": "static/tab_mine_selected.png",
                "text": "我的"
            }
        ]
    },
    "subPackages": [
        {
            "root": "page_subject",
            "pages": [ 
                {
                    "path": "pages/auth",
                    "style": {
                        "navigationBarTitleText": "身份认证"
                    }
                }
            ],
            "preloadRule": {
                "page_subject/pages/auth": {
                    "network": "all",
                    "packages": [
                        "__APP__"
                    ]
                }
            }
        }
    ],
    "uniIdRouter": {}
}